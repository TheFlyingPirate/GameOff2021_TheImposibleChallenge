--item update and draw

--bombs
function add_bomb(x,y,fr)
  add(bombs,{x=x,y=y,fr=fr,force=30})
end

function draw_bombs()
  for b in all(bombs) do
    local bomb_sprite = 17
    if (b.fr-frame)%((b.fr-frame)%5)==0 then
      if bomb_sprite == 17 then
        bomb_sprite = 18
      else
        bomb_sprite = 17
      end
    end
    
    if b.fr<frame then
      --do explossion
      local pdx = b.x-player.x
      local pdy = b.y-player.y
      pdx *= pdx
      pdy *= pdy
      if pdx+pdy<1600 then
        player.health -= 10
      --apply force
        if player.x>b.x then
          if player.flp_x==true then
            player.dx-=b.force
          else
            player.dx+=b.force
          end
        elseif player.x<b.x then
          if player.flp_x==true then
            player.dx+=b.force
          else
            player.dx-=b.force
          end
        end
      
      end
      --damage to enemies
      for e in all(enemy) do
        local dex = e.x-b.x
        local dey = e.y-b.y
        dex *= dex
        dey *= dey
        
        if dex+dey<1600 then
          e.health-=10
        end
      end
      
      sfx(2)
      del(bombs,b)
    else
      spr(bomb_sprite,b.x,b.y,1,1)
    end
  end
end

