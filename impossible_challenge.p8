--enemys
function add_new_enemy(etype,health,x,y,dx,dy,acc)
  add(enemy,{
  enem=etype,
  health=health,
  x=x,
  y=y,
  w=8,
  h=8,
  damage=3,
  dx=dx,
  dy=dy,
  
  flippedx=false,
  flippedy=false})
end
function draw_enemys()
  for e in all(enemy) do
    if e.enem=="ghost" then
      spr(32,e.x,e.y,1,1,e.flippedx,e.flippedy)
    end
  end
end
function update_enemys()
for e in all(enemy) do
    if e.enem=="ghost" then
      if frame%20<10 then
        e.dx+=1
      else
        e.dx-=1
      end
    end
    --collider
    local x0=e.x
    local x1=e.x+e.w
    local y0=e.y
    local y1=e.y+e.h
    if player.x>=x0 and player.x<=x1 and player.y>=y0 and player.y<=y1 then
      player.health-=e.damage
      if e.enem!="bug" and e.enem!="bug_proj" then
        if player.health<0 then
          player.health=0
        end
      else
        if player.health>0 then
          player.health=0
        end
      end
    end
    
    e.x+=e.dx
    e.y+=e.dy 
    e.dx=0
    e.dy=0
    if e.health<=0 then
      del(enemy,e)
    end
   
  end
end




