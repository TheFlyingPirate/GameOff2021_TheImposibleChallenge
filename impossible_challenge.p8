--update and draw
function _update()
  if go==false and level_started==true then
    complete_level()
    player_update()
    player_animate()
  	 update_enemys()
  end
end

function _draw()
  if level==0 then
    start_level1()
  end
  if player.health==0 then
    cls()
    draw_go()
    if go == false then
      music(-1)
      sfx(1)
    end
    go = true
  else
    if level_started==true then
      cls()
      spr(16,0,0,1,1,false)
      print(player.health.."/100",9,2)
      if level==1 then
        map(0,0)
      else
      end
      spr(goal.sp,goal.x,goal.y,1,1)
      spr(player.sp,player.x,player.y,1,1,player.flp_x)
      draw_bombs()
  		  draw_enemys()
    end
  end
  frame+=1
end
function draw_go()
  print("game over",49,49)
 
end

function complete_level()
  if level_comp==true then
    level_comp=false
    cls()
    sfx(5)
    print("level "..level.." completed",30,49)
    level+=1
    level_started=false
    music(-1,300)
    
  end
end


function start_level1()
  music(0)
  goal.x=120
  goal.y=48
  level_started=true
  level=1
  player.x=80
  player.y=0
  add_new_enemy("ghost",10,40,40,0,0)
  add_new_enemy("ghost",10,50,40,0,0)

end

