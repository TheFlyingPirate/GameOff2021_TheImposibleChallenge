--player animate function
function player_animate()
  
  if player.jumping then
    player.sp = 4
  elseif player.running then
    if player.sp == 2 and frame%5==0 then
      player.sp = 3
    elseif frame%5==0 then
      
      player.sp = 2
    end
    
    
  else
    player.sp = 1
  end
end
