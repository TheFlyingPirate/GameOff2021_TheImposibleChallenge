--player update
function player_update()
  --check if goal
  if player.x>=goal.x and player.x<=goal.x+goal.w and player.y>=goal.y and player.y<=goal.y+goal.h then
    level_comp=true
  end
  
  
  --physics
  player.dy+=gravity
  player.dx*=friction
  
  --controls
  if btnp(⬆️) then
    add_bomb(player.x,player.y,frame+100)
  end
  
  if btn(⬅️) then
    player.flp_x = true
    player.dx+=player.acc
    player.running = true
  end
  if btn(➡️) then
    player.flp_x = false
    player.dx+=player.acc
    player.running = true
  end
  if btnp(❎) and player.landed then
    player.dy-=player.jump_acc
    player.jumping = true
    player.landed = false
    sfx(3)
  end
  
  --check collission
  if player.dy>0 then
    player.falling=true
    player.landed=false
    player.jumping= false
    
    if collide_map(player,"down",0) then
      player.landed=true
      player.falling=false
      player.dy=0
      player.y -=(player.y+player.h)%8
    end
   
  elseif player.dy<0 then
    player.jumping=true
    if collide_map(player,"up",1) then
      player.dy=0
    end  
  
  end
  if player.dx<0 then
    if player.flp_x then
      
      if collide_map(player,"right",1) then
        if player.dx >=3 then
          player.health -=1
        end
        player.dx=0
        player.x +=(player.x+player.w)%8
      end
    else
      
      
      if collide_map(player,"left",1) then
      if player.dx>=3 then
        player.health -=1
      end
      player.dx=0
       player.x -=(player.x+player.w)%8
      end
    end
  end
  
  if player.dx>0 then
    if player.flp_x then
      if collide_map(player,"left",1) then
        if player.dx >=3 then
          player.health -=1
        end
        player.dx=0
        player.x +=(player.x+player.w)%8
      end
    else
      if collide_map(player,"right",1) then
      if player.dx>=3 then
        player.health -=1
      end
      player.dx=0
       player.x -=(player.x+player.w)%8
      end
    end
  end
  if player.y>=129 then
    player.health=0
  end
  if player.dx>=player.max_dx then
    player.dx=player.max_dx
  end
  if player.dy>=player.max_dy then
    player.dy=player.max_dy
  end
  if player.dx<=1 then
    player.standing=true
    player.running=false
  end
  
  player.y+=player.dy
  if player.flp_x== true then
    player.x-=player.dx
  else
    player.x+=player.dx
  end
end
